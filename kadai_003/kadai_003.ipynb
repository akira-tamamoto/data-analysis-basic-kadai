        "import numpy as np\n",
        "\n",
        "a = np.array([[0,1], [2,3], [4,5]])\n",
        "b = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "#aとbを計算する\n",
        "np.dot(a, b)\n",
        "\n",
        "#計算結果を代入\n",
        "result = np.dot(a,b)\n",
        "\n",
        "#計算結果を出力\n",
        "print(result)\n",
        "\n",
        "#最大値を知る\n",
        "result.max()\n",
        "\n",
        "#最大値を代入\n",
        "max_number = result.max()\n",
        "\n",
        "#最大値を出力\n",
        "print(max_number)"
